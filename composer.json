{
    "name": "vigihdev/wp-deregister",
    "description": "WordPress Deregister",
    "version": "1.0.1",
    "type": "library",
    "license": "Mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "vigih dev",
            "email": "vigihdev@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.thrubus.id"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "vigihdev/wp-kernel": "dev-main",
        "vigihdev/serializer": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "vigihdev\\WpDeregister\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vigihdev\\WpDeregister\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "dev": "php -S localhost:4377 dev.php"
    },
    "config": {
        "process-timeout": 0
    }
}